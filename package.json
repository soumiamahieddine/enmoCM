{
  "name": "maarchcourrier",
  "version": "21.03.8",
  "description": "MaarchCourrier",
  "scripts": {
    "build-prod": "ng build --prod",
    "build-watch": "ng build --watch",
    "build-prod-addin-outlook": "ng build --project \"addin-outlook\" --prod",
    "build-addin-outlook": "ng build --project \"addin-outlook\"",
    "build": "ng build",
    "build-release": "npm run build-prod && npm run build-prod-addin-outlook && npm run reload-packages-prod && npm run reload-packages-prod-addin-outlook",
    "check-update": "ncu",
    "dep-list": "npm list --depth=0",
    "upgrade": "ncu -u",
    "lint": "eslint -c .eslintrc.js --ext .ts src/frontend/",
    "lint-fix": "eslint -c .eslintrc.js --ext .ts src/frontend/ --fix",
    "reload-packages": "rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm i",
    "reload-packages-addin-outlook": "cd plugins/addin-outlook && rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm i",
    "reload-packages-prod": "rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm i --production",
    "reload-packages-prod-addin-outlook": "cd plugins/addin-outlook && rm -Rf node_modules && rm package-lock.json && npm cache clean --force && npm i --production",
    "create-component": "echo Component name ? && read varname && ng g c $varname --module app --style=scss",
    "create-component-admin": "echo Component name ? && read varname && ng g c administration/$varname --module administration --style=scss",
    "cypress:open": "cypress open --config-file test/e2e/cypress-config.json",
    "cypress:run": "cypress run --browser firefox --headless --config-file test/e2e/cypress-config.json"
  },
  "keywords": [],
  "author": "Maarch",
  "license": "GPL-3.0",
  "dependencies": {
    "angular2-draggable": "2.3.2",
    "core-js": "3.9.0",
    "zone.js": "0.11.3",
    "tinymce-i18n": "20.4.4",
    "tinymce": "5.5.0"
  },
